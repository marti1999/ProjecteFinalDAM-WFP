<Application x:Class="desktopapplication.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:desktopapplication"
             StartupUri="View/logIn.xaml">
    <Application.Resources>
        <BooleanToVisibilityConverter x:Key="BoolToVis"/>
        <ResourceDictionary x:Key="CustomDictionary">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="CustomResources/ResourceDictionary.xaml" />
                <ResourceDictionary Source="CustomResources/ResourceDictionary.CA.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
